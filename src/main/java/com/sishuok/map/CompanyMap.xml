<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sishuok.map.CompanyMap">
    <select id="getList" parameterType="java.util.Map" resultType="com.sishuok.entity.Company">
      select * from dbo.Company
    </select>
    <select id="getCompany" parameterType="java.util.Map" resultType="com.sishuok.entity.Company">
        select * from dbo.Company where CompanyCode = #{code}
    </select>
</mapper>